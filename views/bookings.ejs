<!DOCTYPE html>
<html>
  <head>
    <title>All Bookings</title>
    <link rel='stylesheet' href='/stylesheets/style.css' />
  </head>
  <body>
    <table>
      <tr>
        <th>Email</th>
        <th>Num of tickets</th>
        <th>Movie</th>
        <th>Payment</th>
        <th>Terms</th>
      </tr>
       
      <% for (var booking of bookings) { %>
            <tr>
              <td><%= booking.email %></td>
                <td><%= booking.noOfTicket %></td>
                <td><%= booking.term %></td>
                <td><%= booking.gridRadios %></td>
                <td><%= booking.terms %></td>
            </tr>
        <% } %>
     </table>
  </body>
</html>